{
  "cli": {
    "version": ">= 15.0.12",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "android": {
        "distribution": "internal"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_WEATHER_APP_ID": "YOUR_WEATHER_APP_ID_HERE"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_WEATHER_APP_ID": "YOUR_WEATHER_APP_ID_HERE"
      }
    }
  }
}
